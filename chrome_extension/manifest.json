{
  // Required
  "manifest_version": 2,
  "name": "AWS AD credentials fetcher",
  "version": "1.0",
  // Optional
  "background": {
    "scripts": [
      "parameters.js",
      "background.js",
      "aws-sdk-2.673.min.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://login.microsoftonline.com/common/oauth2/authorize*"
      ],
      "js": [
        "select_user.js"
      ]
    },
    {
      "matches": [
        "https://myapplications.microsoft.com/*"
      ],
      "js": [
        "parameters.js",
        "select_app.js"
      ]
    },
    {
      "matches": [
        "https://signin.aws.amazon.com/saml"
      ],
      "js": [
        "parameters.js",
        "select_role.js"
      ]
    }
  ],
  "permissions": [
    "*://*/*",
    "contentSettings",
    "downloads",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ]
}